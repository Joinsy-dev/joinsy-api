{
  "name": "joinsy-api",
  "version": "1.0.0",
  "description": "Ferramenta para utilização da api joinsy",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joinsy-dev/joinsy-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Joinsy-dev/joinsy-api/issues"
  },
  "homepage": "https://github.com/Joinsy-dev/joinsy-api#readme",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4"
  }
